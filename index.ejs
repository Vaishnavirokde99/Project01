                        <h5>New Issue</h5>
                        <form id="issue-form">
                            <div class="row mb-2">
                                <label class="col-sm-2 col-form-label" for="issue-part">Part #</label>
                                <div class="col-sm-10">
                                    <input class="form-control-plaintext" id="issue-part" type="text" readonly="" value="123">
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-sm-2 col-form-label" for="issue-title">Type</label>
                                <div class="col-sm-10">
                                    <select class="form-control form-control-sm" id="issue-title">
                                        <option>Minor</option>
                                        <option>Major</option>
                                        <option>Urgent</option>
                                        <option>Critical</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-sm-2 col-form-label" for="issue-author">Author</label>
                                <div class="col-sm-10">
                                    <select class="form-control form-control-sm" id="issue-author">
                                        <option>Sherbaum Valery</option>
                                        <option>Erenburg Vladimir</option>
                                        <option>John Golley</option>
                                        <option>Wilbert Awdry</option>
                                        <option>Bill Gunston</option>
                                        <option>Klaus Hünecke</option>
                                        <option>Irwin E. Treager</option>
                                    </select>
                                </div>
                            </div>
