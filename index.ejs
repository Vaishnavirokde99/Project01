                                    <th class="table-data" scope="col">Recommended</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% newData.forEach(item => { %>
                                    <tr>
                                        <td class="table-data"><%= item.Status %></td>
                                        <td class="table-data"><%= item.Author %></td>
