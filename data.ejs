
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data Display</title>
    <a href="/display">display</a>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Data Display</h1>
    <table>
        <thead>
            <tr>
                <th>Status</th>
                <th>Author</th>
                <th>Recommended</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(item => { %>
                <tr>
                    <td><%= item.Status %></td>
                    <td><%= item.Author %></td>
                    <td><%= item.Recommended %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
